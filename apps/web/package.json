{
  "name": "web",
  "version": "1.0.0",
  "description": "Vue 3 web application for VSG Kugelberg",
  "author": "Rene Wagner",
  "license": "ISC",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check \"src/**/*.{js,ts,vue,json,md}\"",
    "format:fix": "prettier --write \"src/**/*.{js,ts,vue,json,md}\"",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^4.0.10",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^6.3.5",
    "vitest": "^4.0.10",
    "vue-tsc": "^2.2.10"
  }
}
